{
  "name": "whirligiggle",
  "version": "0.0.9",
  "description": "accessible, performant, mobile friendly carousel",
  "main": "index.js",
  "scripts": {
    "prestart": "rimraf dist/",
    "start": "npm-run-all -s scss -p server js:dev watch:scss",
    "build": "npm-run-all -s scss js:build",
    "server": "node server",
    "scss": "node-sass src/ -o dist/ --output-style compressed",
    "js:build": "webpack --colors --display-chunks --display-modules -p",
    "js:dev": "webpack --colors --display-chunks -w",
    "watch:scss": "nodemon -e scss -x \"npm run scss\"",
    "test": "jest ./src/spec/*.spec.js"
  },
  "author": "Corinne",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@testing-library/dom": "^7.24.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/user-event": "^12.1.7",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.5.2",
    "nodemon": "^2.0.4",
    "pug-loader": "^2.4.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "express": "^4.17.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.0"
  }
}
